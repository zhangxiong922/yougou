.public {
  width: 100%;
  height: 180rpx;
  display: flex;
}
.public navigator {
  width: 25%;
  height: 180rpx;
 display: flex;
 justify-content: center;
  align-items: center;
}
.public navigator image {
   width: 70%;
   height: 70%;
   vertical-align: middle;
}
.floordata {
  width: 100%;
  box-sizing: border-box;
}
.floordata .list1 {
  width: 100%;
  background-color: rgba(0,0,0,.1);
  padding-top: 10rpx;
  vertical-align: middle;
}
.floordata .list2 {
  box-sizing: border-box;
  padding: 20rpx;
  overflow: hidden;
}
.floordata .list3  {
  width: 33.33%;
  float: left;
  box-sizing: border-box;

}
.floordata .list3:first-child {
  padding-right: 10rpx;
}
.floordata .list3:nth-child(4),
.floordata .list3:nth-child(5) {
  border-top: 10rpx solid #fff;
 
}
.floordata .list3:nth-child(2),
.floordata .list3:nth-child(4) {
  padding-right: 10rpx;
}
.floordata .list3:first-child image {
  width: 100%;
  height: 416rpx;
  vertical-align: middle;
}
.floordata .list3:nth-last-child(-n + 4) image {
  width: 100%;
  height: 208rpx;
  vertical-align: middle;
}